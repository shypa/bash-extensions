# Usual aliases
alias ls="ls --color=auto"
alias ll="ls -l"
alias la="ls -a"
alias lla="ls -la"
alias lh="ls -lh"
alias grep="grep --color=auto"

# User specific aliases and functions
alias clr="clear"
alias la="ls -a"
alias chmods="chmod u+x"

alias b="back"
alias n="next"

alias rsync="rsync -r --progress"
