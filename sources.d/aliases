# Usual aliases
alias ls="ls --color=auto"
alias ll="ls -l"
alias la="ls -a"
alias lla="ls -la"
alias lld="ls -ld"
alias lh="ls -lh"
alias grep="grep --color=auto"

# User specific aliases and functions
alias chmods="chmod u+x"

alias b="back"
alias n="next"

alias rsync="rsync -r --progress"
alias wl-copy="wl-copy --foreground --paste-once --trim-newline"
